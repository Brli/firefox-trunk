<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE default-apps SYSTEM "gnome-da-list.dtd">
<default-apps>
  <web-browsers>
    <web-browser>
      <name>Minefield</name>
      <executable>@APPNAME@</executable>
      <command>@APPNAME@ %s</command>
      <icon-name>@APPNAME@</icon-name>
      <run-in-terminal>false</run-in-terminal>
      <netscape-remote>true</netscape-remote>
      <tab-command>@APPNAME@ -new-tab "%s"</tab-command>
      <win-command>@APPNAME@ -new-window "%s"</win-command>
    </web-browser>
  </web-browsers>
</default-apps>


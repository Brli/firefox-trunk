Name: @BRAND_NAME@ restart required
Name-fr: Relancement de @BRAND_NAME@ requis
Priority: High
Terminal: False
DontShowAfterReboot: True
DisplayIf: pgrep -x @APPNAME@ -U $(id -u) > /dev/null

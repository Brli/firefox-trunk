#!/bin/sh

set -e

APPNAME=@APPNAME@

if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ] ; then
    update-alternatives --remove gnome-www-browser /usr/bin/$APPNAME
    update-alternatives --remove x-www-browser /usr/bin/$APPNAME
fi

if [ -f /usr/share/apport/package-hooks/$APPNAME.pyc ] ; then
    rm -f /usr/share/apport/package-hooks/$APPNAME.pyc
fi

#DEBHELPER#
